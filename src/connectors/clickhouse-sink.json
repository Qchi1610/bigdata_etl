{
  "name": "clickhouse-sink",
  "config": {
    "connector.class": "com.clickhouse.kafka.connect.ClickHouseSinkConnector",
    "topics": "dbserver1.public.products,dbserver1.public.categories,dbserver1.public.orders,dbserver1.public.customers",
    "topic2TableMap": "dbserver1.public.products=products,dbserver1.public.categories=categories,dbserver1.public.orders=orders,dbserver1.public.customers=customers",
    "tasks.max": "1",
    "hostname": "clickhouse",
    "port": "8123",
    "database": "inventory",
    "username": "default",
    "password": "",
    "ssl": "false",
    "exactly_once": "true",
    "key.converter": "org.apache.kafka.connect.storage.StringConverter",
    "value.converter": "org.apache.kafka.connect.json.JsonConverter",
    "value.converter.schema.registry.url": "http://schema-registry:8081",
    "value.converter.schemas.enable": "true",
    "transforms": "castTypes",
    "transforms.castTypes.type": "org.apache.kafka.connect.transforms.Cast$Value",
    "transforms.castTypes.spec": "is_active:int8"
  }
}